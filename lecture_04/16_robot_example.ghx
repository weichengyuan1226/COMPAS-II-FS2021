                    <item name="CodeInput" type_name="gh_string" type_code="10">import os
from scriptcontext import sticky as st
import compas


from compas.robots import RobotModel
from compas.robots import LocalPackageMeshLoader
from compas_fab.ghpython import RobotArtist
from compas_fab.robots import RobotSemantics as RobotSemantics

from compas_fab.robots import Robot as Robot
from compas_fab.backends import RosClient

# Set high precision to import meshes defined in meters
compas.PRECISION = '12f'

# store data in component
guid = str(ghenv.Component.InstanceGuid)
robot_key = "robot_" + guid


if robot_key not in st:
    st[robot_key] = None


if ros_client and ros_client.is_connected and load == True:
    
    model = RobotModel.from_urdf_file(os.path.join(urdf_source, 'robot_description.urdf'))
    model.load_geometry(LocalPackageMeshLoader(urdf_source, 'robot_description'))
    semantics = RobotSemantics.from_srdf_file(os.path.join(urdf_source, 'robot_description.srdf'), model)
    artist = RobotArtist(model)
    robot = Robot(model, artist, semantics)
    
    robot.scale(scale_factor)
    st[robot_key] = robot

robot = st[robot_key]

if robot:
    robot.client = ros_client

